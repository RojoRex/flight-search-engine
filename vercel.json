{
    "rewrites": [
        {
            "source": "/amadeus/v1/:match*",
            "destination": "https://test.api.amadeus.com/v1/:match*"
        },
        {
            "source": "/amadeus/v2/:match*",
            "destination": "https://test.api.amadeus.com/v2/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}